<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<!--分类显示-->


<AnchorPane prefHeight="650.0" prefWidth="400.0" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="a.eve.newbox.controller.ClassifiController">
   <children>
      <ListView fx:id="data_list" prefHeight="550.0" prefWidth="400.0" style="-fx-background-color: pink;" />
      <TextField fx:id="add_text" layoutX="25.0" layoutY="555.0" prefHeight="30.0" prefWidth="350.0" promptText="添加分类" style="-fx-background-color: lightblue; -fx-background-radius: 15;">
         <font>
            <Font size="20.0" />
         </font>
      </TextField>
      <Button fx:id="add_button" layoutX="50.0" layoutY="600.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: #57d26d; -fx-background-radius: 10;" text="添加">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
      <Button fx:id="ok_button" layoutX="250.0" layoutY="600.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="100.0" style="-fx-background-color: white; -fx-background-radius: 10;" text="确定" textAlignment="CENTER">
         <font>
            <Font size="19.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
